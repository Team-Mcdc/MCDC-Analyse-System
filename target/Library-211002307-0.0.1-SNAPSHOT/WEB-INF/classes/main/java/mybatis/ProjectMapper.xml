<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.ProjectFileDao">
    <insert id="addProjectFile" parameterType="po.Project">
        INSERT INTO project_files (UserId,name, project_path)
        VALUES (#{UserId},#{name}, #{project_file})
    </insert>
</mapper>
